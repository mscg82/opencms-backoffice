<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
 <description>The backoffice / JSP additional tag library.
        Developed by Mashfrog for OpenCms.</description>
 <display-name>OpenCms Backoffice JSP additional taglib</display-name>
 <tlib-version>1.0</tlib-version>
 <short-name>bko</short-name>
 <uri>http://www.mashfrog.org/taglib/backoffice</uri>
 <function>
  <description>Checks if the provided string is empty or white space only</description>
  <display-name>isEmptyOrWhiteSpaceOnly</display-name>
  <name>isEmptyOrWhiteSpaceOnly</name>
  <function-class>com.mashfrog.backoffice.jsp.util.JspUtil</function-class>
  <function-signature>java.lang.String isEmptyOrWhiteSpaceOnly(java.lang.String)</function-signature>
 </function>
 <function>
  <description>Checks if the provided string is not empty or white space only</description>
  <display-name>isNotEmptyOrWhiteSpaceOnly</display-name>
  <name>isNotEmptyOrWhiteSpaceOnly</name>
  <function-class>com.mashfrog.backoffice.jsp.util.JspUtil</function-class>
  <function-signature>java.lang.String isNotEmptyOrWhiteSpaceOnly(java.lang.String)</function-signature>
 </function>
 <function>
  <display-name>trimToSize</display-name>
  <name>trimToSize</name>
  <function-class>com.mashfrog.backoffice.jsp.util.JspUtil</function-class>
  <function-signature>java.lang.String trimToSize(java.lang.String, int)</function-signature>
 </function>
 <function>
  <display-name>toUpperCase</display-name>
  <name>toUpperCase</name>
  <function-class>com.mashfrog.backoffice.jsp.util.JspUtil</function-class>
  <function-signature>java.lang.String toUpperCase(java.lang.String)</function-signature>
 </function>
 <function>
  <display-name>toLowerCase</display-name>
  <name>toLowerCase</name>
  <function-class>com.mashfrog.backoffice.jsp.util.JspUtil</function-class>
  <function-signature>java.lang.String toLowerCase(java.lang.String)</function-signature>
 </function>
 <function>
  <description>Escapes special char with their HTML entities</description>
  <display-name>escapeHTML</display-name>
  <name>escapeHTML</name>
  <function-class>com.mashfrog.backoffice.jsp.util.JspUtil</function-class>
  <function-signature>java.lang.String escapeHTML(java.lang.String)</function-signature>
 </function>
</taglib>
